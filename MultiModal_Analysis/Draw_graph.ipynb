    "                #df = df.iloc[::500, :]\n",
    "                df.index = pd.to_timedelta(df.index / fps, unit='s')  # Convert index to timedelta format based on fps\n",
    "                df.index = df.index.astype(str).str.split(' ').str[-1]  # Convert timedelta to string and keep only the time part\n",
    "                temp[person] = df\n",
    "\n",
